<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Calculator Suite">
    <test name="Math test">
        <groups>
            <run>
                <include name="math"></include>
            </run>
        </groups>
        <classes>
            <class name="org.example.CalculatorGroupTest"></class>
        </classes>
    </test>
    <test name="Division test">
        <groups>
            <run>
                <include name="division"></include>
            </run>
        </groups>
        <classes>
            <class name="org.example.CalculatorGroupTest"></class>
        </classes>
    </test>
</suite>